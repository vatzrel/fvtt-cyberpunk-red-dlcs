{"name":"Bottle dynamo","type":"itemUpgrade","img":"modules/cp-red-dlcs/assets/spinning-your-wheels/gear.webp","system":{"weaponType":"assaultRifle","weaponSkill":"Archery","fireModes":{"autoFire":0,"suppressiveFire":false},"damage":"1d6","rof":1,"dvTable":"","isRanged":false,"unarmedAutomaticCalculation":true,"attackmod":0,"tags":[],"description":{"value":"<p><span id=\"page10R_mcid314\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">A generator attaches to the wheels, allowing you to </span></span><span id=\"page10R_mcid315\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">generate enough electrical power to charge a single </span></span><span id=\"page10R_mcid316\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">battery pack, of the sort used to power Microwavers </span></span><span id=\"page10R_mcid317\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">and similar weapons, with an hour&rsquo;s ride.</span></span></p>","chat":"","unidentified":""},"source":"Spinning Your Wheels","favorite":false,"concealable":{"concealable":false,"isConcealed":false},"install":"mall","isInstalled":false,"size":1,"usesType":"magazine","magazine":{"value":0,"max":0,"ammoId":""},"ammoVariety":[],"price":{"market":20},"handsReq":0,"type":"gear","modifiers":{"secondaryWeapon":{"configured":"false"},"damage":{"type":"modifier","value":null},"rof":{"type":"modifier","value":null},"attackmod":{"type":"modifier","value":null},"magazine":{"type":"modifier","value":null}}},"effects":[],"flags":{},"_stats":{"systemId":"cyberpunk-red-core","systemVersion":"v0.84.0","coreVersion":"10.284","createdTime":1669832355035,"modifiedTime":1669833268623,"lastModifiedBy":"Kv4DkZZX1tRJvylk"},"_id":"7aIL4O8YwJqK4lT4","folder":null,"sort":0,"ownership":{"default":0,"Kv4DkZZX1tRJvylk":3}}
{"name":"Trailer","type":"itemUpgrade","img":"modules/cp-red-dlcs/assets/spinning-your-wheels/gear.webp","system":{"weaponType":"assaultRifle","weaponSkill":"Archery","fireModes":{"autoFire":0,"suppressiveFire":false},"damage":"1d6","rof":1,"dvTable":"","isRanged":false,"unarmedAutomaticCalculation":true,"attackmod":0,"tags":[],"description":{"value":"<p><span id=\"page13R_mcid447\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">A small trailer you can hook onto your bicycle with a </span></span><span id=\"page13R_mcid448\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">cargo area measuring 5 feet (1.5 meters) long and 1.5 </span></span><span id=\"page13R_mcid449\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">feet (.46 meters) wide. It can be attached or detached </span></span><span id=\"page13R_mcid450\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">from the bicycle with an Action.</span></span></p>\n<p><span id=\"page13R_mcid451\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">If the bicycle has the Folding Frame upgrade, the </span></span><span id=\"page13R_mcid452\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">trailer must be detached before it is folded.</span></span></p>","chat":"","unidentified":""},"source":"Spinning Your Wheels","favorite":false,"concealable":{"concealable":false,"isConcealed":false},"install":"mall","isInstalled":false,"size":1,"usesType":"magazine","magazine":{"value":0,"max":0,"ammoId":""},"ammoVariety":[],"price":{"market":50},"handsReq":0,"type":"gear","modifiers":{"secondaryWeapon":{"configured":"false"},"damage":{"type":"modifier","value":null},"rof":{"type":"modifier","value":null},"attackmod":{"type":"modifier","value":null},"magazine":{"type":"modifier","value":null}}},"effects":[],"flags":{},"_stats":{"systemId":"cyberpunk-red-core","systemVersion":"v0.84.0","coreVersion":"10.284","createdTime":1669832355035,"modifiedTime":1669833268627,"lastModifiedBy":"Kv4DkZZX1tRJvylk"},"_id":"CKmCuYQ7SS2gMbbo","folder":null,"sort":0,"ownership":{"default":0,"Kv4DkZZX1tRJvylk":3}}
{"name":"Gun Mount","type":"itemUpgrade","img":"modules/cp-red-dlcs/assets/spinning-your-wheels/gear.webp","system":{"weaponType":"assaultRifle","weaponSkill":"Archery","fireModes":{"autoFire":0,"suppressiveFire":false},"damage":"1d6","rof":1,"dvTable":"","isRanged":false,"unarmedAutomaticCalculation":true,"attackmod":0,"tags":[],"description":{"value":"<p><span id=\"page10R_mcid379\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">A mount for an Assault Rifle, Heavy SMG, or Shotgun </span></span><span id=\"page10R_mcid380\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">is attached to the bicycle&rsquo;s handlebars, complete with a </span></span><span id=\"page10R_mcid381\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">trigger button on the grip. You do not need a free hand </span></span><span id=\"page10R_mcid382\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">to fire the weapon but it can only shoot forward. You </span></span><span id=\"page10R_mcid383\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">cannot reload while in transit. Attaching and detaching </span></span><span id=\"page10R_mcid384\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">a weapon takes an Action.</span></span></p>","chat":"","unidentified":""},"source":"Spinning Your Wheels","favorite":false,"concealable":{"concealable":false,"isConcealed":false},"install":"mall","isInstalled":false,"size":1,"usesType":"magazine","magazine":{"value":0,"max":0,"ammoId":""},"ammoVariety":[],"price":{"market":100},"handsReq":0,"type":"gear","modifiers":{"secondaryWeapon":{"configured":"false"},"damage":{"type":"modifier","value":null},"rof":{"type":"modifier","value":null},"attackmod":{"type":"modifier","value":null},"magazine":{"type":"modifier","value":null}}},"effects":[],"flags":{},"_stats":{"systemId":"cyberpunk-red-core","systemVersion":"v0.84.0","coreVersion":"10.284","createdTime":1669832355035,"modifiedTime":1669833268624,"lastModifiedBy":"Kv4DkZZX1tRJvylk"},"_id":"I4UkQNNuvVBceg2N","folder":null,"sort":0,"ownership":{"default":0,"Kv4DkZZX1tRJvylk":3}}
{"name":"Security System","type":"itemUpgrade","img":"modules/cp-red-dlcs/assets/spinning-your-wheels/gear.webp","system":{"weaponType":"assaultRifle","weaponSkill":"Archery","fireModes":{"autoFire":0,"suppressiveFire":false},"damage":"1d6","rof":1,"dvTable":"","isRanged":false,"unarmedAutomaticCalculation":true,"attackmod":0,"tags":[],"description":{"value":"<p><span id=\"page13R_mcid413\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">When activated by an app on your Agent, the bicycle </span></span><span id=\"page13R_mcid414\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">is electrified. Anyone attempting to grab it must </span></span><span id=\"page13R_mcid415\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">succeed at a DV13 Resist Torture/Drugs Check or </span></span><span id=\"page13R_mcid416\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">release the bike immediately.</span></span></p>\n<p><span class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\"><span id=\"page13R_mcid417\" class=\"markedContent\">The bicycle is also registered with a nano-VIN and</span><span id=\"page13R_mcid418\" class=\"markedContent\"><br role=\"presentation\">Yang&rsquo;s Wheels guarantees they&rsquo;ll call you and return </span><span id=\"page13R_mcid419\" class=\"markedContent\">it, at no charge, should someone steal it and attempt </span><span id=\"page13R_mcid420\" class=\"markedContent\">to sell it to them. They&rsquo;ll even rebuild it, free of charge, </span><span id=\"page13R_mcid421\" class=\"markedContent\">if someone brings it in as parts.</span></span></span></p>","chat":"","unidentified":""},"source":"Spinning Your Wheels","favorite":false,"concealable":{"concealable":false,"isConcealed":false},"install":"mall","isInstalled":false,"size":1,"usesType":"magazine","magazine":{"value":0,"max":0,"ammoId":""},"ammoVariety":[],"price":{"market":500},"handsReq":0,"type":"gear","modifiers":{"secondaryWeapon":{"configured":"false"},"damage":{"type":"modifier","value":null},"rof":{"type":"modifier","value":null},"attackmod":{"type":"modifier","value":null},"magazine":{"type":"modifier","value":null}}},"effects":[],"flags":{},"_stats":{"systemId":"cyberpunk-red-core","systemVersion":"v0.84.0","coreVersion":"10.284","createdTime":1669832355035,"modifiedTime":1669833268625,"lastModifiedBy":"Kv4DkZZX1tRJvylk"},"_id":"LZMRynhxdgt5v0T1","folder":null,"sort":0,"ownership":{"default":0,"Kv4DkZZX1tRJvylk":3}}
{"name":"Extended Seat","type":"itemUpgrade","img":"modules/cp-red-dlcs/assets/spinning-your-wheels/gear.webp","system":{"weaponType":"assaultRifle","weaponSkill":"Archery","fireModes":{"autoFire":0,"suppressiveFire":false},"damage":"1d6","rof":1,"dvTable":"","isRanged":false,"unarmedAutomaticCalculation":true,"attackmod":0,"tags":[],"description":{"value":"<p><span id=\"page10R_mcid362\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">A longer-than-normal seat is mounted onto the bicycle, </span></span><span id=\"page10R_mcid363\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">allowing the bicycle to carry a passenger behind the rider.</span></span></p>","chat":"","unidentified":""},"source":"Spinning Your Wheels","favorite":false,"concealable":{"concealable":false,"isConcealed":false},"install":"mall","isInstalled":false,"size":1,"usesType":"magazine","magazine":{"value":0,"max":0,"ammoId":""},"ammoVariety":[],"price":{"market":20},"handsReq":0,"type":"gear","modifiers":{"secondaryWeapon":{"configured":"false"},"damage":{"type":"modifier","value":null},"rof":{"type":"modifier","value":null},"attackmod":{"type":"modifier","value":null},"magazine":{"type":"modifier","value":null}}},"effects":[],"flags":{},"_stats":{"systemId":"cyberpunk-red-core","systemVersion":"v0.84.0","coreVersion":"10.284","createdTime":1669832355035,"modifiedTime":1669833268624,"lastModifiedBy":"Kv4DkZZX1tRJvylk"},"_id":"M5r6urEv1TTXRJSm","folder":null,"sort":0,"ownership":{"default":0,"Kv4DkZZX1tRJvylk":3}}
{"name":"Skateboard","type":"gear","img":"modules/cp-red-dlcs/assets/spinning-your-wheels/skateboard.webp","system":{"tags":[],"description":{"value":"<p><span id=\"page13R_mcid483\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">A deck, two trucks, and four wheels and a staple of </span></span><span id=\"page13R_mcid484\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">extreme sports enthusiasts the world over.</span></span></p>\n<p><em><span class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\"><span id=\"page13R_mcid485\" class=\"markedContent\">In addition to allowing the rider to perform sick tricks, </span><span id=\"page13R_mcid486\" class=\"markedContent\">skateboards increase movement by 4 m/yds when </span><span id=\"page13R_mcid487\" class=\"markedContent\">using the Run Action so long as it is being used on level </span><span id=\"page13R_mcid488\" class=\"markedContent\">or downward sloping ground.</span></span></span></em></p>\n<p><em><span class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\"><span class=\"markedContent\"><span id=\"page13R_mcid489\" class=\"markedContent\">At the GM&rsquo;s discretion, performing a physical task </span><span id=\"page13R_mcid490\" class=\"markedContent\">involving the legs or balance while on a skateboard </span><span id=\"page13R_mcid491\" class=\"markedContent\">might count as a Complex Task (</span><span id=\"page13R_mcid492\" class=\"markedContent\">see CP:R page 130</span><span id=\"page13R_mcid493\" class=\"markedContent\">) </span><span id=\"page13R_mcid494\" class=\"markedContent\">and impose a -2 modifier to Checks.</span></span></span></span></em></p>","chat":"","unidentified":""},"source":"Spinning Your Wheels","favorite":false,"revealed":true,"usage":"toggled","equipped":"owned","concealable":{"concealable":false,"isConcealed":false},"amount":1,"upgrades":[],"slots":0,"isUpgraded":false,"price":{"market":50}},"effects":[],"flags":{},"_stats":{"systemId":"cyberpunk-red-core","systemVersion":"v0.84.0","coreVersion":"10.284","createdTime":1669831964448,"modifiedTime":1669833268623,"lastModifiedBy":"Kv4DkZZX1tRJvylk"},"_id":"SIUQluRTDVLUlvnY","folder":null,"sort":0,"ownership":{"default":0,"Kv4DkZZX1tRJvylk":3}}
{"name":"Bicycle","type":"vehicle","img":"modules/cp-red-dlcs/assets/spinning-your-wheels/bicycle.webp","system":{"tags":"","description":{"value":"<p>Bicycle speed depends on BODY stat:</p>\n<table style=\"width: 100%; height: 51px;\" border=\"1\"><colgroup><col style=\"width: 59.949%;\"><col style=\"width: 19.898%;\"><col style=\"width: 19.898%;\"></colgroup>\n<tbody>\n<tr>\n<td>BODY</td>\n<td>Speed (Combat)</td>\n<td>Speed (Narrative)</td>\n</tr>\n<tr style=\"height: 17px;\">\n<td style=\"height: 17px;\">BODY&lt;4</td>\n<td style=\"height: 17px;\">8 MOVE</td>\n<td style=\"height: 17px;\">12 MPH/19 KPH</td>\n</tr>\n<tr style=\"height: 17px;\">\n<td style=\"height: 17px;\">BODY 4 to 7</td>\n<td style=\"height: 17px;\">10 MOVE</td>\n<td style=\"height: 17px;\">20 MPH/32 KPH</td>\n</tr>\n<tr style=\"height: 17px;\">\n<td style=\"height: 17px;\">BODY 8+</td>\n<td style=\"height: 17px;\">15 MOVE</td>\n<td style=\"height: 17px;\">30 MPH/48 KPH</td>\n</tr>\n</tbody>\n</table>\n<p><span id=\"page4R_mcid250\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">Damage for crashing/ramming is 3d6 for the </span></span><span id=\"page4R_mcid251\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">bicycle and the target struck, instead of 6d6. In </span></span><span id=\"page4R_mcid252\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">addition to suffering the Whiplash Critical Injury, </span></span><span id=\"page4R_mcid253\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">the rider is thrown off the bike and considered </span></span><span id=\"page4R_mcid254\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">Prone</span></span></p>","chat":"","unidentified":""},"source":"Spinning Your Wheels","favorite":false,"concealable":{"concealable":false,"isConcealed":false},"upgrades":[],"slots":1,"isUpgraded":false,"price":{"market":100},"sdp":15,"seats":1,"speedCombat":0,"speedNarrative":""},"effects":[],"flags":{},"_stats":{"systemId":"cyberpunk-red-core","systemVersion":"v0.84.0","coreVersion":"10.284","createdTime":1669830327695,"modifiedTime":1669833268622,"lastModifiedBy":"Kv4DkZZX1tRJvylk"},"_id":"VOO3ht4dKjlhFMyV","folder":null,"sort":0,"ownership":{"default":0,"Kv4DkZZX1tRJvylk":3}}
{"name":"Electric Pedal Assist","type":"itemUpgrade","img":"modules/cp-red-dlcs/assets/spinning-your-wheels/gear.webp","system":{"weaponType":"assaultRifle","weaponSkill":"Archery","fireModes":{"autoFire":0,"suppressiveFire":false},"damage":"1d6","rof":1,"dvTable":"","isRanged":false,"unarmedAutomaticCalculation":true,"attackmod":0,"tags":[],"description":{"value":"<p><span id=\"page10R_mcid340\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">A small motor is added to the bicycle, making it almost </span></span><span id=\"page10R_mcid341\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">effortless to pedal. Increase the bicycle&rsquo;s Speed </span></span><span id=\"page10R_mcid342\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">(Narrative) to the next BODY tier or add 5 MPH (8 </span></span><span id=\"page10R_mcid343\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">KPH) to Speed (Narrative) if it is at the highest tier. In </span></span><span id=\"page10R_mcid344\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">addition, the rider gains a +4 to Endurance Checks </span></span><span id=\"page10R_mcid345\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">made to resist exhaustion after long rides.</span></span></p>","chat":"","unidentified":""},"source":"Spinning Your Wheels","favorite":false,"concealable":{"concealable":false,"isConcealed":false},"install":"mall","isInstalled":false,"size":1,"usesType":"magazine","magazine":{"value":0,"max":0,"ammoId":""},"ammoVariety":[],"price":{"market":100},"handsReq":0,"type":"gear","modifiers":{"secondaryWeapon":{"configured":"false"},"damage":{"type":"modifier","value":null},"rof":{"type":"modifier","value":null},"attackmod":{"type":"modifier","value":null},"magazine":{"type":"modifier","value":null}}},"effects":[],"flags":{},"_stats":{"systemId":"cyberpunk-red-core","systemVersion":"v0.84.0","coreVersion":"10.284","createdTime":1669832355035,"modifiedTime":1669833268623,"lastModifiedBy":"Kv4DkZZX1tRJvylk"},"_id":"bg96eFnrn6J5T8sK","folder":null,"sort":0,"ownership":{"default":0,"Kv4DkZZX1tRJvylk":3}}
{"name":"Neon Lighting","type":"itemUpgrade","img":"modules/cp-red-dlcs/assets/spinning-your-wheels/gear.webp","system":{"weaponType":"assaultRifle","weaponSkill":"Archery","fireModes":{"autoFire":0,"suppressiveFire":false},"damage":"1d6","rof":1,"dvTable":"","isRanged":false,"unarmedAutomaticCalculation":true,"attackmod":0,"tags":[],"description":{"value":"<p><span id=\"page13R_mcid390\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">Battery-powered fiber-optic neon lighting is integrated </span></span><span id=\"page13R_mcid391\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">into your bicycle&rsquo;s frame and wheels to add some </span></span><span id=\"page13R_mcid392\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">serious style to its substance. This counts as 1 installa</span></span><span id=\"page13R_mcid394\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">tion of a Light Tattoo when riding, sitting on, or being </span></span><span id=\"page13R_mcid395\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">near your bike, for the purposes of the +2 Wardrobe </span></span><span id=\"page13R_mcid396\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">and Style bonus.</span></span></p>","chat":"","unidentified":""},"source":"Spinning Your Wheels","favorite":false,"concealable":{"concealable":false,"isConcealed":false},"install":"mall","isInstalled":false,"size":1,"usesType":"magazine","magazine":{"value":0,"max":0,"ammoId":""},"ammoVariety":[],"price":{"market":20},"handsReq":0,"type":"gear","modifiers":{"secondaryWeapon":{"configured":"false"},"damage":{"type":"modifier","value":null},"rof":{"type":"modifier","value":null},"attackmod":{"type":"modifier","value":null},"magazine":{"type":"modifier","value":null}}},"effects":[],"flags":{},"_stats":{"systemId":"cyberpunk-red-core","systemVersion":"v0.84.0","coreVersion":"10.284","createdTime":1669832355035,"modifiedTime":1669833268625,"lastModifiedBy":"Kv4DkZZX1tRJvylk"},"_id":"ctP8PDbW4mteLNXe","folder":null,"sort":0,"ownership":{"default":0,"Kv4DkZZX1tRJvylk":3}}
{"name":"Cycle Armor","type":"itemUpgrade","img":"modules/cp-red-dlcs/assets/spinning-your-wheels/gear.webp","system":{"weaponType":"assaultRifle","weaponSkill":"Archery","fireModes":{"autoFire":0,"suppressiveFire":false},"damage":"1d6","rof":1,"dvTable":"","isRanged":false,"unarmedAutomaticCalculation":true,"attackmod":0,"tags":[],"description":{"value":"<p><span id=\"page10R_mcid332\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">Plating and reinforcement is added to your bicycle, </span></span><span id=\"page10R_mcid333\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">making it more resistant to damage. It now has 7 SP. </span></span><strong><span id=\"page10R_mcid334\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">Requires the Reinforced Frame upgrade.</span></span></strong></p>","chat":"","unidentified":""},"source":"Spinning Your Wheels","favorite":false,"concealable":{"concealable":false,"isConcealed":false},"install":"mall","isInstalled":false,"size":1,"usesType":"magazine","magazine":{"value":0,"max":0,"ammoId":""},"ammoVariety":[],"price":{"market":100},"handsReq":0,"type":"gear","modifiers":{"secondaryWeapon":{"configured":"false"},"damage":{"type":"modifier","value":null},"rof":{"type":"modifier","value":null},"attackmod":{"type":"modifier","value":null},"magazine":{"type":"modifier","value":null}}},"effects":[],"flags":{},"_stats":{"systemId":"cyberpunk-red-core","systemVersion":"v0.84.0","coreVersion":"10.284","createdTime":1669832355035,"modifiedTime":1669833268623,"lastModifiedBy":"Kv4DkZZX1tRJvylk"},"_id":"dtvwpREW0iDThVJU","folder":null,"sort":0,"ownership":{"default":0,"Kv4DkZZX1tRJvylk":3}}
{"name":"Smuggling Compartment","type":"itemUpgrade","img":"modules/cp-red-dlcs/assets/spinning-your-wheels/gear.webp","system":{"weaponType":"assaultRifle","weaponSkill":"Archery","fireModes":{"autoFire":0,"suppressiveFire":false},"damage":"1d6","rof":1,"dvTable":"","isRanged":false,"unarmedAutomaticCalculation":true,"attackmod":0,"tags":[],"description":{"value":"<p><span id=\"page13R_mcid427\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">A small, cleverly concealed storage space is built into </span></span><span id=\"page13R_mcid428\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">the frame. It can hold objects up to 1 inch x 6 inches </span></span><span id=\"page13R_mcid429\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">(2.54cm x 15.24cm). Discovering it requires a DV 17 </span></span><span id=\"page13R_mcid430\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">Conceal/Reveal Object Check.</span></span><span id=\"page13R_mcid431\" class=\"markedContent\"> <strong><span dir=\"ltr\" role=\"presentation\">Incompatible with </span></strong></span><strong><span id=\"page13R_mcid432\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">the Folding Frame upgrade.</span></span></strong></p>","chat":"","unidentified":""},"source":"Spinning Your Wheels","favorite":false,"concealable":{"concealable":false,"isConcealed":false},"install":"mall","isInstalled":false,"size":1,"usesType":"magazine","magazine":{"value":0,"max":0,"ammoId":""},"ammoVariety":[],"price":{"market":50},"handsReq":0,"type":"gear","modifiers":{"secondaryWeapon":{"configured":"false"},"damage":{"type":"modifier","value":null},"rof":{"type":"modifier","value":null},"attackmod":{"type":"modifier","value":null},"magazine":{"type":"modifier","value":null}}},"effects":[],"flags":{},"_stats":{"systemId":"cyberpunk-red-core","systemVersion":"v0.84.0","coreVersion":"10.284","createdTime":1669832355035,"modifiedTime":1669833268625,"lastModifiedBy":"Kv4DkZZX1tRJvylk"},"_id":"eH3mUSymCN5H1hVX","folder":null,"sort":0,"ownership":{"default":0,"Kv4DkZZX1tRJvylk":3}}
{"name":"Enclosure","type":"itemUpgrade","img":"modules/cp-red-dlcs/assets/spinning-your-wheels/gear.webp","system":{"weaponType":"assaultRifle","weaponSkill":"Archery","fireModes":{"autoFire":0,"suppressiveFire":false},"damage":"1d6","rof":1,"dvTable":"","isRanged":false,"unarmedAutomaticCalculation":true,"attackmod":0,"tags":[],"description":{"value":"<p><span id=\"page10R_mcid351\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">The bicycle is upgraded to a trike and adds a frame </span></span><span id=\"page10R_mcid352\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">and enclosing fiberglass and plastic walls around the </span></span><span id=\"page10R_mcid353\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">rider to keep out the weather. If you have the Cycle </span></span><span id=\"page10R_mcid354\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">Armor upgrade it extends to the enclosure walls. </span></span><strong><span id=\"page10R_mcid356\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">Incompatible with the Folding Frame upgrade.</span></span></strong></p>","chat":"","unidentified":""},"source":"Spinning Your Wheels","favorite":false,"concealable":{"concealable":false,"isConcealed":false},"install":"mall","isInstalled":false,"size":1,"usesType":"magazine","magazine":{"value":0,"max":0,"ammoId":""},"ammoVariety":[],"price":{"market":100},"handsReq":0,"type":"gear","modifiers":{"secondaryWeapon":{"configured":"false"},"damage":{"type":"modifier","value":null},"rof":{"type":"modifier","value":null},"attackmod":{"type":"modifier","value":null},"magazine":{"type":"modifier","value":null}}},"effects":[],"flags":{},"_stats":{"systemId":"cyberpunk-red-core","systemVersion":"v0.84.0","coreVersion":"10.284","createdTime":1669832355035,"modifiedTime":1669833268623,"lastModifiedBy":"Kv4DkZZX1tRJvylk"},"_id":"fQRIGQvx75h5a4AP","folder":null,"sort":0,"ownership":{"default":0,"Kv4DkZZX1tRJvylk":3}}
{"name":"Inline Skates","type":"gear","img":"modules/cp-red-dlcs/assets/spinning-your-wheels/inline-skates.webp","system":{"tags":[],"description":{"value":"<p><span id=\"page13R_mcid465\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">A pair of roller skates with four wheels arranged in a </span></span><span id=\"page13R_mcid466\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">single line.</span></span></p>\n<p><em><span class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\"><span id=\"page13R_mcid467\" class=\"markedContent\">Inline Skates increase movement by 4 m/yds when </span><span id=\"page13R_mcid468\" class=\"markedContent\">using the Run Action. Pulling the skates on or taking </span><span id=\"page13R_mcid469\" class=\"markedContent\">them off requires an Action. Any options stored in </span><span id=\"page13R_mcid470\" class=\"markedContent\">a Cyberleg or meat leg are inaccessible while the </span><span id=\"page13R_mcid471\" class=\"markedContent\">skates are worn.</span><span id=\"page13R_mcid472\" class=\"markedContent\"><br role=\"presentation\">At the GM&rsquo;s discretion, performing a physical task </span><span id=\"page13R_mcid473\" class=\"markedContent\">involving the legs or balance while wearing skates </span><span id=\"page13R_mcid474\" class=\"markedContent\">might count as a Complex Task </span><span id=\"page13R_mcid477\" class=\"markedContent\">and impose a -2 modifier to Checks.</span></span></span></em></p>","chat":"","unidentified":""},"source":"Spinning Your Wheels","favorite":false,"revealed":true,"usage":"toggled","equipped":"owned","concealable":{"concealable":false,"isConcealed":false},"amount":1,"upgrades":[],"slots":0,"isUpgraded":false,"price":{"market":50}},"effects":[],"flags":{},"_stats":{"systemId":"cyberpunk-red-core","systemVersion":"v0.84.0","coreVersion":"10.284","createdTime":1669831762053,"modifiedTime":1669833268623,"lastModifiedBy":"Kv4DkZZX1tRJvylk"},"_id":"lxmEK31Sw8dmOxoo","folder":null,"sort":0,"ownership":{"default":0,"Kv4DkZZX1tRJvylk":3}}
{"name":"Folding Frame","type":"itemUpgrade","img":"modules/cp-red-dlcs/assets/spinning-your-wheels/gear.webp","system":{"weaponType":"assaultRifle","weaponSkill":"Archery","fireModes":{"autoFire":0,"suppressiveFire":false},"damage":"1d6","rof":1,"dvTable":"","isRanged":false,"unarmedAutomaticCalculation":true,"attackmod":0,"tags":[],"description":{"value":"<p><span id=\"page10R_mcid369\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">The bicycle can now be folded into an easy-to-carry/stow </span></span><span id=\"page10R_mcid370\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">package roughly the size of a large briefcase.</span></span><span id=\"page10R_mcid371\" class=\"markedContent\"> <strong><span dir=\"ltr\" role=\"presentation\">Incompatible </span></strong></span><strong><span id=\"page10R_mcid372\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">with Enclosure, the Reinforced Frame, and </span></span><span id=\"page10R_mcid373\" class=\"markedContent\"><span dir=\"ltr\" role=\"presentation\">Smuggling Compartment upgrades.</span></span></strong></p>","chat":"","unidentified":""},"source":"Spinning Your Wheels","favorite":false,"concealable":{"concealable":false,"isConcealed":false},"install":"mall","isInstalled":false,"size":1,"usesType":"magazine","magazine":{"value":0,"max":0,"ammoId":""},"ammoVariety":[],"price":{"market":100},"handsReq":0,"type":"gear","modifiers":{"secondaryWeapon":{"configured":"false"},"damage":{"type":"modifier","value":null},"rof":{"type":"modifier","value":null},"attackmod":{"type":"modifier","value":null},"magazine":{"type":"modifier","value":null}}},"effects":[],"flags":{},"_stats":{"systemId":"cyberpunk-red-core","systemVersion":"v0.84.0","coreVersion":"10.284","createdTime":1669832355035,"modifiedTime":1669833268624,"lastModifiedBy":"Kv4DkZZX1tRJvylk"},"_id":"oAjxuytY8HOV1UHv","folder":null,"sort":0,"ownership":{"default":0,"Kv4DkZZX1tRJvylk":3}}
